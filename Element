public class Element <T> {
	
	private T value;
	private Element<T> next;
	
	public Element(T value)
	{
		this.value = value;
		this.next = null;
	}
	
	public void setValue(T value)
	{
		this.value = value;
	}
	
	public T getValue()
	{
		return value;
	}
	
	public void setNext(Element<T> e)
	{
		this.next = e;
	}
	
	public Element<T> getNext()
	{
		return next;
	}
}
